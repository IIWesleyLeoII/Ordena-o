#
inclua biblioteca Util 
	funcao inicio(){
	inteiro vetor[5], i, aux
	
	para( i = 0; i < 5; i++){
		vetor[i] = Util.sorteia( 1, 100)
	}
	para( i = 0; i < 5; i++){
		escreva(vetor[i], " ")
	}
	escreva("\n")
	para(i = 0; i < 5 - 1; i++){
		se(vetor[i] > vetor[i+1]){
			aux = vetor[i]
			vetor[i] = vetor[i+1]
			vetor[i+1] = aux
		}
	}
	para(i = 0; i < 5; i++){
		escreva(vetor[i], " ")
	}
	
	}
